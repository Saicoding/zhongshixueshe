/* components/errorRecovery/errorRecovery.wxss */

.container {
  width: 690rpx;
  position: fixed;
  top: 200rpx;
  left: 30rpx;
  z-index: 100;
}

/* 头部 */

.up {
  display: flex;
  justify-content: center;
  width: 690rpx;
  height: 114rpx;
  background-size: 690rpx 114rpx;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.up-text {
  letter-spacing: 5rpx;
  color: white;
  line-height: 110rpx;
  font-size: 35rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.up-line {
  width: 50rpx;
  border-top: 1rpx solid white;
  margin-top: 55rpx;
}

/* 中间部分 */

.mid {
  width: 100%;
  background: white;
  padding-top: 30rpx;
  padding-bottom: 40rpx;
}

.mid-img {
  width: 200rpx;
  height: 194rpx;
  display: block;
  margin: 0 auto;
}

.mid-text {
  width: 100%;
  height: 70rpx;
  line-height: 50rpx;
  font-size: 25rpx;
  text-align: center;
  color: #595959;
}

.mid-btn {
  width: 400rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  color: white;
  font-size: 28rpx;
  text-align: center;
  margin: 0 auto;
  background: #31cb97;
}

.btn{
  width: 500rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  border:none;
  color: white;
  text-align: center;
  font-size: 25rpx;
  margin: 0 auto;
  margin-top:30rpx;
  background: #fd635b;
}

/* 下面部分 */

.down {
  width: 690rpx;
  padding: 40rpx 0rpx 60rpx 0rpx;
  background: #ecf7e9;
  border-radius: 0 0 20rpx 20rpx;
}

.down-img {
  display: block;
  width: 630rpx;
  height: 145rpx;
  margin: 0 auto;
}

.down-text {
  width: 100%;
  height: 90rpx;
  line-height: 60rpx;
  font-size: 22rpx;
  text-align: center;
  color: #545a50;
}

.down-text1 {
  width: 630rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  text-align: center;
  color: #545a50;
  padding-top:20rpx;
  padding-left:30rpx;
  padding-right:30rpx;
}

.down-btn {
  width: 500rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  color: white;
  text-align: center;
  font-size: 25rpx;
  margin: 0 auto;
  background: #fd635b;
}

.modal {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  filter: Alpha(Opacity=60);
  background-color: #1f1f1f;
  opacity: 0.5;
  z-index: 99;
}

.select1{
  background: #90c5b4;
}

.select2{
  background: #fa9a95;
}
