/* components/errorRecovery/errorRecovery.wxss */

.container {
  position: fixed;
  width: 660rpx;
  top: 250rpx;
  left: 45rpx;
  z-index: 100;
}

.close{
  float: right;
  width:60rpx;
  height:60rpx;
  border-radius: 60rpx;
  border:1rpx solid rgba(245, 245, 245,0.5);
  background:rgba(10, 10, 10,0.3);
  margin-bottom: 30rpx;
}

.close-icon{
  width:24rpx;
  height:24rpx;
  margin-top:19rpx;
  margin-left:19rpx;
}

.content{
  position: relative;
  background: white;
  border-radius: 10rpx;
  top:-110rpx;
  padding-bottom:20rpx;
  z-index: 100;
}

.rili{
  position: relative;
  z-index: 101;
  width:180rpx;
  height:164rpx;
  font-size:40rpx;
  background-size: 180rpx 164rpx;
  text-align: center;
  line-height: 204rpx;
  color:#ea542b;
  letter-spacing: 6rpx;
  margin:0 auto;
  margin-top:50rpx;
  font-weight: bolder;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.headpics{
  display: flex;
  width:100%;
  justify-content: center;
  margin:0 auto;
  margin-top:30rpx;
}

.headpics-icon{
  width: 60rpx;
  height: 60rpx;
  border-radius: 60rpx;
  margin-right: 10rpx;
  box-shadow: -5rpx 3rpx 3rpx 4rpx rgba(0, 0, 0, 0.1);
  margin-left: -20rpx;
}

.content-text{
  display: flex;
  justify-content: space-between;
  font-size:25rpx;
  color:#bdbdbd;
  height:120rpx;
  line-height: 110rpx;
}

/* 中间部分 */
.mid-line{
  width:580rpx;
  margin:0 auto;
  border-top:1rpx dashed #e6e6e6;
}

.swiper-container{
  width:660rpx;
  overflow: hidden;
}

.swiper{
  width:700rpx;
  height:300rpx;
}

.qiandao-item{
  padding-top:50rpx;
  width:140rpx;
}

.qiandao-item-up{
  width:95rpx;
  height:112rpx;
  margin:0 auto;
  background-size:95rpx 112rpx;
  background-repeat: no-repeat;
}

.qiandao-item-up-text{
  color:#c7c7c7;
  font-size:23rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
}

.qiandao-item-up-text1{
  color:white;
  font-size:23rpx;
  height:160rpx;
  line-height: 160rpx;
  text-align: center;
}

.qiandao-item-up-icon{
  text-align: center;
  color:white;
  font-size:20rpx;
  height:50rpx;
  line-height: 60rpx;
}

.qiandao-item-mid{
  display: flex;
  margin-top:30rpx;
}

.qiandao-item-mid-line{
  width:60rpx;
  border-top:1rpx solid #efefef;
  margin-top:10rpx;
}

.qiandao-item-mid-circle{
  width:20rpx;
  height:20rpx;
  border-radius: 20rpx;
   background: radial-gradient(#50bc57 40%, #84d186 30%, #84d186 30%); 
   background: radial-gradient(#d8d8d8 40%, #f3f3f3 30%, #f1f1f1 30%); 
}

.qiandao-item-down{
  font-size:20rpx;
  text-align: center;
  margin-top:20rpx;
}

.btn{
  width:300rpx;
  height:65rpx;
  line-height: 65rpx;
  border-radius: 65rpx;
  color:white;
  font-size:28rpx;
  text-align: center;
  background: linear-gradient(to bottom,#4cb54e,#78cf7a);
  margin:30rpx auto;
  box-shadow: 3rpx 8rpx 8rpx #4db64f;
}

.select1{
  background: linear-gradient(to bottom,#08960b,#22ac24);
  box-shadow: 3rpx 8rpx 8rpx #22ac24;
}

.modal {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  filter: Alpha(Opacity=60);
  background: rgba(0, 0, 0, 0.7);
  z-index: 99;
}