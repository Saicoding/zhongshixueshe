/* components/errorRecovery/errorRecovery.wxss */

.container {
  position: fixed;
  width: 630rpx;
  top: 250rpx;
  left: 60rpx;
  z-index: 100;
  background: white;
  border-radius: 10rpx;
}

.modal {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  filter: Alpha(Opacity=60);
  background: rgba(12, 12, 12, 0.3);
  z-index: 99;
}

.title {
  width: 100%;
  height: 80rpx;
  line-height: 115rpx;
  font-size: 32rpx;
  text-align: center;
  color: black;
}

.num {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #7a7a7a;
  font-size: 22rpx;
  text-align: center;
}

/* 中间部分 */

.mid {
  display: flex;
  justify-content: space-around;
  width: 100%;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.mid-item {
  color: white;
  width: 150rpx;
  text-align: center;
}

.mid-item-1 {
  font-size: 50rpx;
  margin-top: 40rpx;
}

.mid-item-2 {
  font-size: 25rpx;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}

/* 选项 */

.selects {
  display: flex;
  justify-content: space-around;
  margin-top: 50rpx;
}

.select, .selectitem {
  height: 40rpx;
  text-align: center;
  font-size: 25rpx;
  line-height: 40rpx;
}

.select {
  width: 187rpx;
}

.selectitem {
  width: 90rpx;
  border-radius: 40rpx;
  margin-left: 30rpx;
  margin: 0 auto;
}

/* 滑块 */

.switch {
  display: flex;
  justify-content: center;
  margin-top: 50rpx;
}

.switch-label {
  width: 200rpx;
  font-size: 28rpx;
  text-align: center;
  height: 80rpx;
  line-height: 60rpx;
}

.submit {
  background: #31d684;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
  text-align: center;
  color: white;
  font-size: 25rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin-top: 80rpx;
}

.select1{
  background: #9adbbb;
}