/* pages/login/login.wxss */
.bg{
  position: relative;
  z-index:50;
  width:750rpx;
  height:402rpx;
}

.mycontainer{
  position: absolute;
    z-index:100;
  top:300rpx;
  padding-bottom:50rpx;
}

.bounder{
  width:650rpx;
  margin-left:50rpx;
  border-radius: 15rpx;
  background: white;
  box-shadow: 3rpx 6rpx 15rpx rgba(141, 141, 141,0.2);
}

.top{
  justify-content: space-between;
  border-radius: 15rpx;
  background: white;
}

.top-text{
  font-size:30rpx;
}

.top-item{
  width:350rpx;
  text-align: center;
  height:80rpx;
  line-height: 90rpx;
}

.top-line{
  width:70rpx;
  height:10rpx;
  border-radius: 10rpx;
  margin-left:127rpx;
}

/* 输入框 */
.content{
  margin-top:60rpx;
  font-size:28rpx;
  height:450rpx;
}
.block{
  width:560rpx;
  margin-left:45rpx;
  margin-top:20rpx;
  height:80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid rgba(237, 237, 237,0.7);
}

.block-input{
  height:80rpx;
  line-height: 80rpx;
  width:300rpx;
  margin-left:40rpx;
}

.block-icon{
  width:40rpx;
  height:40rpx;
  margin-top:20rpx;
}

.eye{
  width:80rpx;
  height:80rpx;
}

.eye-icon{
  width:40rpx;
  height:40rpx;
  margin-top:20rpx;
  margin-left:20rpx;
}

/* 登录按钮 */
.submit{
  width:560rpx;
  height:80rpx;
  line-height: 80rpx;
  font-size:32rpx;
  text-align: center;
  margin: 0 auto;
  border-radius: 80rpx;
  color:white;
  letter-spacing: 7rpx;
  margin-top:60rpx;
}

.forget{
  height:100rpx;
  width:100%;
  text-align: center;
  line-height: 100rpx;
  color:#ec7323;
  letter-spacing: 5rpx;
  font-size:25rpx;
}

/* 快速登录的线 */
.line{
  display: flex;
  justify-content: center;
}

.line-border{
  width:180rpx;
  border-top:1rpx solid rgba(204, 204, 204,0.3);
  margin-top:15rpx;
}

.line-text{
  height:40rpx;
  color:rgb(187, 187, 187);
  font-size:22rpx;
}

.identify-code{
  width:200rpx;
  height:50rpx;
  line-height: 50rpx;
  font-size:25rpx;
  text-align: center;
  border-radius: 50rpx;
  color:white;
  margin-top:15rpx;
  background:#f88415;
}


.subClass1{
    background: linear-gradient(to right,#d3d3d3,#969696);
  box-shadow: 3rpx 6rpx 14rpx#d3d3d3;
}

.subClass2{
  background: linear-gradient(to right,#f49f22,#f56a20);
  box-shadow: 3rpx 6rpx 14rpx#f39e22;
}

/* 快捷登录的图标 */
.fast{
  width:750rpx;
  margin-top:50rpx;
}
.fastlogin{
  display: flex;
  justify-content: center;
  margin-top:40rpx;
}

.fastlogin-button{
  width:65rpx;
  height:65rpx;
  border-radius: 65rpx;
  margin-left:80rpx;
   background-size:65rpx 65rpx;
  background-image: url('data:image/png;base64,');
}

.fastlogin-button::after{
  border: none;
}

.fastlogin-icon{
  width:65rpx;
  height:65rpx;
}

.select1{
  opacity: 0.3;
}

.select2{
  background: linear-gradient(to right,#fa7000,#fa3200);
}

