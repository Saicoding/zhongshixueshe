/* pages/video/videoDetail/videoDetail.wxss */

.video, .video-img {
  width: 750rpx;
  height: 422rpx;
  z-index: 10;
}

.wifi {
  position: absolute;
  top: 0rpx;
  width: 750rpx;
  height: 422rpx;
  background: black;
  color: white;
  text-align: center;
  z-index: 1;
}

.unBuy {
  position: absolute;
  background: black;
  width: 750rpx;
  height: 422rpx;
  color: white;
  text-align: center;
  z-index: 1;
}

.unBuy-text {
  font-size: 35rpx;
  margin-top: 150rpx;
}

.wifi-text1, .wifi-text2 {
  width: 100%;
}

.wifi-text1 {
  font-size: 30rpx;
  letter-spacing: 2rpx;
  margin-top: 90rpx;
}

.wifi-text2 {
  font-size: 25rpx;
  margin-top: 30rpx;
  letter-spacing: 1rpx;
}

.wifi-button {
  width: 180rpx;
  height: 55rpx;
  font-size: 25rpx;
  line-height: 55rpx;
  border: 1rpx solid #b74e2a;
  border-radius: 5rpx;
  margin: 80rpx auto;
  color: #b74e2a;
}

.video-play {
}

.video-icon {
  width: 90rpx;
  height: 90rpx;
  margin: 150rpx auto;
}

.up-block {
  width: 96%;
  padding: 2%;
  border-top: 1px #ddd solid;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 100;
}

.xk {
  margin-right: 20rpx;
  border: 1px #ddd solid;
  padding: 0 15rpx;
  border-radius: 3px;
  color: #aaa;
  font-size: 20rpx;
  line-height: 35rpx;
}

.xk.on {
  color: #fff;
  background: #d3d3d3;
  border: 1px #d3d3d3 solid;
}

.mr10 {
  margin-right: 15px;
}

.text {
  display: inline-block;
}

.hong {
  color: #ee804c;
}

.kc_money, .tag1, .tag2, .youhui, .tag3, .purchased {
  display: float;
  float: left;
}

.kc_money {
  width: 32%;
  text-align: center;
  font-size: 42rpx;
  line-height: 40rpx;
  color: #ee804c;
}

.kc_money text {
  margin-right: 4rpx;
  font-weight: bold;
}

.tag1, .tag2, .tag3 {
  font-size: 25rpx;
  width: 60rpx;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  margin-top: 12rpx;
  color: white;
  border-radius: 3rpx;
}

.tag1 {
  background: #0197f6;
}

.tag2 {
  margin-left: 20rpx;
  background: rgb(42, 212, 42);
}

.tag3 {
  margin-left: 20rpx;
  background: #fc7b03;
}

.zixun {
  width: 28%;
  background: none;
  border-radius: 0;
  border-right: 2px #ddd solid;
  height: 44rpx;
  line-height: 44rpx;
  margin-top: 18rpx;
}

.zixun image {
  width: 46rpx;
  height: 44rpx;
}

.youhui, .purchased {
  width: 35%;
  font-size: 34rpx;
  background: #63d56b;
  line-height: 80rpx;
  color: white;
  float: right;
  text-align: center;
  border-radius: 5rpx;
}

.purchased {
  background: #eee;
  color: #999;
}

.scrolltop {
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border: 1px #eee solid;
  border-radius: 4rpx;
  position: fixed;
  bottom: 100px;
  right: 10px;
  z-index: 10;
  background: #f7f8f9;
  -webkit-box-shadow: 0 0 6px 1px #ddd;
  box-shadow: 0 0 6px 1px #ddd;
  display: none;
}

.scrolltop.on {
  display: block;
}

.mybar {
  border-top: 20rpx solid #f8f8f8;
  background: #fff;
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx solid #e5e5e5;
  position: relative;
}

.mybar.on {
  border-top: 0;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}

.title-block1 {
  width: 520rpx;
  margin: 0 auto;
}

.beisu-container {
  display: flex;
  right:0;
  position: absolute;
  z-index:100;
}

.beisu {
  width: 4em;
  text-align: center;
  border: 1px rgba(238, 238, 238,0.3) solid;
  background: rgba(255, 255, 255, 0.3);
  font-size: 22rpx;
  border-radius: 4px;
  line-height: 38rpx;
  color: #999;
  margin-right: 10px;
  float: right;
  margin-top: 5px;
}

.beisu.on {
  background: rgba(255, 153, 0, 0.2);
  color: #fff;
  border: 1px rgba(252, 177, 65,0.2) solid;
}

.hasNopage-text{
  color:rgb(153, 153, 153);
  text-align: center;
  font-size:25rpx;
  margin-top:20rpx;
}

.title1 {
  display: float;
  float: left;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 33.3%;
  text-align: center;
}

.blockMove {
  position: absolute;
  bottom: -3rpx;
  left: 45%;
  width: 10%;
  border-bottom: 6rpx solid #66d68e;
  border-radius: 6rpx;
}

.option {
  clear: both;
  margin-top: 30rpx;
}

.info {
  text-align: center;
}

.video-block {
  width: 100%;
}

.myCanvas {
  position: absolute;
  left: -200rpx;
  width: 40rpx;
  height: 40rpx;
  color: rgb(231, 230, 230);
}

.blank {
  width: 40rpx;
  height: 40rpx;
  margin-left: 60rpx;
  margin-top: 32rpx;
}

.blank-in {
  width: 30rpx;
  height: 30rpx;
  border: 1rpx solid #ecebeb;
  border-radius: 30rpx;
  margin-top: 4rpx;
  margin-left: 4rpx;
}

.blank-in-in {
  width: 20rpx;
  height: 20rpx;
  background: #ecebeb;
  border-radius: 20rpx;
  margin-top: 6rpx;
  margin-left: 6rpx;
}

.icon {
  width: 40rpx;
  height: 40rpx;
  margin-left: 60rpx;
  margin-top: 32rpx;
}

.select {
  color: #66d68e;
}

.title-block {
  width: 94%;
  margin: 0 auto;
  border-bottom: 2px #eee solid;
  padding: 25rpx 0;
  position: relative;
}

.title-block-img {
  position: absolute;
  right: 0;
  top: 26px;
}

.title-block-tiao {
  position: absolute;
  bottom: -2px;
  border-bottom: 2px #66d68e solid;
}

.title {
  font-size: 28rpx;
  width: 600rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.view-time {
  color: #aaa;
  font-size: 20rpx;
  margin-top: 20rpx;
}

.bottomkk {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0rpx;
  border-top: 1rpx solid #ddd;
  background: #f4f5f6;
}

.bottom-price, .bottom-buy {
  display: float;
  float: left;
}

.bottom-price {
  width: 30%;
  height: 80rpx;
  text-align: center;
  font-size: 40rpx;
  color: #dc3f0c;
}

.bottom-buy {
  width: 70%;
  height: 80rpx;
  background: #2693ff;
  color: white;
  text-align: center;
  font-size: 35rpx;
}

.pllist {
  width: 94%;
  padding: 4% 3%;
  border-bottom: 1px #eee solid;
}

.plimg {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  float: left;
}

.plcont {
  width: 85%;
  float: right;
}

.plcont .clear {
  line-height: 35px;
}

.pltext {
  line-height: 24px;
  color: #555;
}

.pltext2 {
  padding: 2%;
  background: #f4f7f9;
}

.pltext2 text {
  color: #64d56b;
}

/* 评论载入框 */
.loading-block {
  width: 100%;
  height: 50rpx;
  font-size: 23rpx;
  line-height: 53rpx;
  margin: 0rpx auto;
  text-align: center;
  color: rgb(156, 156, 156);
}

.loading-gif {
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: middle;
}

.loading-text {
  display: inline-block;
  margin-left: 10rpx;
  vertical-align: middle;
}


/* 最下面部分 */

.bottom {
  position: fixed;
  display: flex;
  bottom: 0rpx;
  background: #f5f6f7;
  height: 100rpx;
  width: 100%;
}

.bottom-icon {
  width: 60rpx;
  height: 60rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.bottom-input {
  background: white;
  border: 1px #eee solid;
  width:520rpx;
  border-radius: 10rpx;
  margin-left: 20rpx;
  height: 70rpx;
  margin-top: 15rpx;
  padding-left: 15rpx;
  font-size: 26rpx;
  float: left;
}

.bottom-send {
  color: white;
  font-size: 25rpx;
  text-align: center;
  border-radius: 12rpx;
  height: 70rpx;
  width: 150rpx;
  margin-top: 15rpx;
  background: #07c160;
  line-height: 70rpx;
  margin-right: 15rpx;
  float: right;
}

.select1{
  background: rgba(163, 163, 163,0.1);
}

.select2{
  background:#00c40d;
}
