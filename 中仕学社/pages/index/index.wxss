/**index.wxss**/

/* 最上面部分 */
.topBar-left {
   display: flex;
  width: 200rpx;
  height: 50rpx;
  border-radius: 0rpx 50rpx 50rpx 0rpx;
  line-height: 50rpx;
  color: white;
  font-size: 22rpx;
  background: linear-gradient(to right, #fcc4c3, #fab2af);
}

.topbar-left-icon{
  width:30rpx;
  height:30rpx;
  margin:10rpx 20rpx 10rpx 40rpx;
}

.topBar-mid {
  letter-spacing: 5rpx;
  height: 50rpx;
  margin-left: 30rpx;
  line-height: 50rpx;
  font-size: 25rpx;
}

.topBar-mid-num {
  color: white;
  font-size: 28rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  padding-left:4rpx;
  padding-right:4rpx;
  background: linear-gradient(to right,#faa4aa,#fe5c64);
  border-radius: 5rpx;
  letter-spacing: 0rpx;
  margin:7rpx;
  margin-right:10rpx;
}

.topBar-right{
  width: 90rpx;
  height: 90rpx;
  background-image: url('data:image/png;base64,');
  background-size: 90rpx;
  background-repeat: no-repeat;
  color: #fc771f;
  line-height: 105rpx;
  font-size: 25rpx;
  margin-right:10rpx;
  text-indent: 23rpx;
}

/* 中间的banner */
.swiper,.swiper-image{
  width:750rpx;
  height:283rpx;
}

.swiper{
  margin-top:15rpx;
}

/* 目录列表 */
.catlogList{
  background: #f8f8f8;
  padding-top:10rpx;
  padding-bottom:10rpx;

}
.catlog-item{
  float: left;
  width:186rpx;
  padding-top:30rpx;
  padding-bottom:20rpx;
  border-radius: 10rpx;
}

.catlog-item-icon{
  width:50rpx;
  height:50rpx;
  margin-left:68rpx;
}

.catlog-item-text{
  text-align: center;
  font-size:25rpx;
  margin-top:10rpx;
}

/* 中间部分 */

.info {
  width:750rpx;
  left: 25rpx;
  background: white;
  padding: 30rpx 0rpx 40rpx 0rpx;
}

.info-up {
  display: flex;
  height: 80rpx;
  padding-left:30rpx;
}

.info-up-avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
}

.info-up-mid, .info-up-name, .info-up-jihua {
  width: 450rpx;
}

.info-up-mid {
  height: 80rpx;
  margin-left: 20rpx;
}

.info-up-jihua {
  height: 60rpx;
  color: #818181;
  font-size: 25rpx;
}

.info-up-name {
  line-height: 30rpx;
  color: #818181;
  font-size: 25rpx;
}

.info-up-jihua {
  display: flex;
  line-height: 60rpx;
}

.info-up-jihua-icon {
  width: 26rpx;
  height: 26rpx;
  margin-top: 17rpx;
}

.info-up-jihua-text {
  margin-left: 10rpx;
  font-size:22rpx;
}

.info-up-right{
  height:40rpx;
  line-height: 40rpx;
  font-size:22rpx;
  color:#f97c2c;
}

.info-up-right-icon{
  width:16rpx;
  height:16rpx;
  margin-top:12rpx;
  margin-left:10rpx;
}


.info-down {
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}

.info-down-item {
  display: flex;
}

.info-down-item-icon {
  width: 50rpx;
  height: 50rpx;
  margin-top: 20rpx;
}

.info-down-item-mid {
  height: 90rpx;
}

.info-down-item-mid, .info-down-item-num, .info-down-item-midtext {
  width: 100rpx;
}

.info-down-item-midtext {
  color: #818181;
  font-size: 22rpx;
  text-align: center;
}

.info-down-item-num {
  font-size: 50rpx;
  text-align: center;
  color: black;
}

.info-down-item-text {
  font-size: 22rpx;
  color: rgb(99, 99, 99);
}

/* 继续刷题 继续看课 */

.mid {
  width: 710rpx;
  display: flex;
  justify-content: space-around;
  padding: 0rpx 20rpx;
}

.mid-item {
  width: 330rpx;
  box-shadow: 1rpx 1rpx 13rpx 7rpx rgba(0, 0, 0, 0.1);
  border-radius: 15rpx;
  height: 230rpx;
}

.mid-item-image {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 30rpx;
  background-size: 150rpx 50rpx;
  color: white;
  text-align: center;
  font-size: 25rpx;
  border-radius: 0 50rpx 50rpx 0;
  background: linear-gradient(to right,#fc9b5f,#f8b890);
}

.mid-item-title {
  font-size: 22rpx;
  text-align: center;
  width: 100%;
  margin-top: 20rpx;
  height: 75rpx;
}

.mid-item-bottom {
  width: 40rpx;
  height: 40rpx;
  margin-left: 275rpx;
}

/* 考试资讯 */
.zixun{
  width:690rpx;
 border-top:1rpx solid rgba(75, 75, 75,0.1);
  padding:10rpx 30rpx 20rpx 30rpx;
  margin-top:30rpx;
}
.zixun-top{
  height:60rpx;
  line-height: 60rpx;
  justify-content: space-between;
  color:black;
  font-weight:bold;
  font-size:28rpx;
}

.zixun-top-icon{
  width:16rpx;
  height:16rpx;
  margin-top:22rpx;
  margin-left:15rpx;
}

.zixun-down{
  margin-top:10rpx;
}

.zixun-down-img{
  width:200rpx;
  height:120rpx;
  border-radius: 10rpx;
}

.zixun-down-text{
  width:460rpx;
  margin-left:30rpx;
}

.zixun-down-text-title{
  font-size:25rpx; 
}

.zixun-down-text-content{
  height:70rpx;
  line-height: 40rpx;
  font-size:22rpx;
  color:rgb(165, 165, 165);
  margin-top:15rpx;
}

/* 课程 */
.ke{
  float: left;
  width:330rpx;
}

.ke-img{
  width:330rpx;
  height:198rpx;
  border-radius: 8rpx;
}

.ke-title{
  font-size:25rpx;
  margin-top:15rpx;
}

.ke-bottom{
  height:60rpx;
  line-height: 60rpx;
  color:#bfbfbf;
}

.ke-bottom-icon{
  width:26rpx;
  height:26rpx;
  margin-top:17rpx;
}

.ke-bottom-text{
  font-size:20rpx;
  margin-left:15rpx;
  width:120rpx;
}

.select1{
   background: linear-gradient(to right, #f8dfdf, #facccb);
}

.select2{
  background:rgba(0, 0, 0,0.1);
}

.select3{
  opacity: 0.2
}


